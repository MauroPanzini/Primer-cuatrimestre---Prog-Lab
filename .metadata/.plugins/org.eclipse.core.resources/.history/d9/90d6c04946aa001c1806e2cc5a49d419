/*
 Ejercicio 2-1: Ingresar 5 números enteros calcular y mostrar el promedio de los números. Probar la
aplicación con distintos valores.
Ejemplo 1: 3 - 5 - 7 - 9 - 1
Ejemplo 2: 2 - 1 - 8 -1 - 2============================================================================
 */

#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int uno;
	int dos;
	int tres;
	int cuatro;
	int cinco;
	int suma;
	int promedio;
	printf("Introduzca el primer número: ");
	scanf("%d" , &uno);
	printf("Introduzca el segundo número: ");
	scanf("%d" , &dos);
	printf("Introduzca el tercer número: ");
	scanf("%d" , &tres);
	printf("Introduzca el cuarto número: ");
	scanf("%d" , &cuatro);
	printf("Introduzca el quinto número: ");
	scanf("%d" , &cinco);
	suma = uno+dos+tres+cuatro+cinco;
	promedio = suma/5;
	printf("El promedio es: %d", promedio);
	return 0;
}
